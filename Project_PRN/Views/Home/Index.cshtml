@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Home Page";
}
<div class="row">
@foreach (var item in ViewData["slist"] as List<Subject>)
{
    
        
            <div class="col-sm-4">
                <div class="card" style="width: 18rem;">
                    <img class="card-img-top" src="https://www.generationsforpeace.org/wp-content/uploads/2018/03/empty.jpg" alt="Card image cap">
                    <div class="card-body">
                        <h5 class="card-title">@item.Name</h5>
                    <a asp-controller="Home" asp-action="ShowQuestion" asp-route-id="@item.Id" class="btn btn-primary">Detail</a>
                    </div>
                </div>
            </div>
        
    
}
</div>

